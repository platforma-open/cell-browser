{
  "name": "@platforma-open/milaboratories.cell-browser-2",
  "version": "1.0.4",
  "scripts": {
    "build": "rm -rf ./block-pack && block-tools pack",
    "mark-stable": "block-tools mark-stable -r 's3://milab-euce1-prod-pkgs-s3-block-registry/pub/releases/?region=eu-central-1'",
    "prepublishOnly": "block-tools pack && block-tools publish -r 's3://milab-euce1-prod-pkgs-s3-block-registry/pub/releases/?region=eu-central-1'"
  },
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@platforma-sdk/model": "catalog:",
    "@platforma-open/milaboratories.cell-browser-2.workflow": "workspace:*",
    "@platforma-open/milaboratories.cell-browser-2.model": "workspace:*",
    "@platforma-open/milaboratories.cell-browser-2.ui": "workspace:*"
  },
  "block": {
    "components": {
      "workflow": "@platforma-open/milaboratories.cell-browser-2.workflow/dist/tengo/tpl/main.plj.gz",
      "model": "@platforma-open/milaboratories.cell-browser-2.model/dist/model.json",
      "ui": "@platforma-open/milaboratories.cell-browser-2.ui/dist"
    },
    "meta": {
      "title": "Cell Browser",
      "logo": "file:../logos/block-logo.png",
      "url": "https://github.com/platforma-open/cell-browser",
      "support": "mailto:support@milaboratories.com",
      "description": "Interactive visualization of single-cell RNA-seq data with UMAP embeddings and gene expression violin plots",
      "longDescription": "file:../docs/description.md",
      "changelog": "file:../CHANGELOG.md",
      "tags": [
        "RNA-seq",
        "visualization",
        "single-cell",
        "annotation"
      ],
      "organization": {
        "name": "MiLaboratories Inc",
        "url": "https://milaboratories.com/",
        "logo": "file:../logos/organization-logo.png"
      },
      "marketplaceRanking": 12800
    }
  },
  "devDependencies": {
    "@platforma-sdk/block-tools": "catalog:"
  }
}
